{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "modules": true
        }
    },
    "rules": {
        "no-mixed-spaces-and-tabs": 1,
        "indent": 0,
        "array-bracket-spacing": 0,
        "semi": 0,
        "eol-last": 0,
        "no-var": 0,
        "no-unused-vars": 0,
        "no-console": 0
    }
}